<header>
    <% if !logged_in? %>
    <% end %>
  </header>
<h1>LOGIN</h1>
<%= form_for :session, url: sign_in_path do |f| %>
<div class="login">
  <div class="container">
    <div class="form">
    <div class="field">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true %>
    </div>

    <div class="field">
      <%= f.label :senha %><br />
      <%= f.password_field :password, autocomplete: "off" %>
    </div>
    <div class="actions">
      <%= f.submit "Entrar", class: "btn-submit" %>
    </div>
  </div>
  </div>
  </div>
  <p>Novo dicente? <%= link_to "Inscreva-se agora!", new_dicente_path %></p>
  <% end %>
